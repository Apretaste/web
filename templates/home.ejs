<div class="row">
	<div class="col s12">
		<h1>M&aacute;s visitados</h1>
	</div>

	<% sites.forEach(function(site){ %>
		<a href="#!" onclick="apretaste.send({'command':'WEB', data:{query:'<%= site.url %>'}})">
			<div class="col s6 m4 l3">
				<div class="card-panel green">
					<span class="white-text"><%= cut(site.title || site.url) %></span>
				</div>
			</div>
		</a>
	<% }) %>
</div>
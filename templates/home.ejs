<div class="row">
	<div class="col s12 input">
		<div class="input-field">
			<input id="query" type="text" class="validate">
			<i class="material-icons green-text small send search" onclick="submit()">send</i>
			<label class="active" for="query">Inserte texto o p√°gina a buscar</label>
		</div>
	</div>
</div>

<div class="row">
	<% sites.forEach(function(item) { %>
		<a href="#!" onclick="apretaste.send({command:'WEB', data:{query:'<%= item.domain %>'}})">
			<div class="col s6 m4 l3">
				<div class="card-panel green">
					<span class="white-text">
						<i class="small material-icons">web</i> <br/>
						<%= item.domain %>
					</span>
				</div>
			</div>
		</a>
	<% }) %>

	<a href="#!" onclick="apretaste.send({command:'WEB HISTORY'})">
		<div class="col s6 m4 l3">
			<div class="card-panel history grey">
				<span class="white-text">
					<i class="medium material-icons">access_time</i>
				</span>
			</div>
		</div>
	</a>
</div>